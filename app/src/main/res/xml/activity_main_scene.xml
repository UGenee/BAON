<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/svg_image3_view"
               motion:framePosition="0"
               android:scaleX="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/svg_image3_view"
               motion:framePosition="0"
               android:scaleX="15" />
           <KeyAttribute
               motion:motionTarget="@+id/svg_image3_view"
               motion:framePosition="0"
               android:scaleY="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/svg_image3_view"
               motion:framePosition="40"
               android:scaleX="15" />
           <KeyAttribute
               motion:motionTarget="@+id/svg_image3_view"
               motion:framePosition="40"
               android:scaleY="15" />
           <KeyAttribute
               motion:motionTarget="@+id/svg_image3_view"
               motion:framePosition="100"
               android:scaleX="15" />
           <KeyAttribute
               motion:motionTarget="@+id/svg_image3_view"
               motion:framePosition="100"
               android:scaleX="50" />

           <KeyAttribute
               motion:motionTarget="@+id/svg_image3_view"
               motion:framePosition="100"
               android:scaleY="50" />
       </KeyFrameSet>
        <OnSwipe motion:maxAcceleration="0.1" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/svg_image_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:rotationX="10"
            android:rotationY="10"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"
            android:scaleX="10"
            android:scaleY="0"
            android:alpha="1" />
        <Constraint
            android:id="@+id/svg_image_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:rotationX="0"
            android:rotationY="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp" />
        <Constraint
            android:id="@+id/svg_image2_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:rotationX="0"
            android:rotationY="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"
            android:scaleX="0"
            android:scaleY="0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/svg_image_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:rotationX="10"
            android:rotationY="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"
            android:scaleX="0"
            android:scaleY="0" />
        <Constraint
            android:id="@+id/svg_image2_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:rotationX="0"
            android:rotationY="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp" />
    </ConstraintSet>
</MotionScene>