<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/tools">
    <Transition motion:constraintSetEnd="@+id/end" motion:constraintSetStart="@id/start" motion:duration="2000">
        <KeyFrameSet>
            <KeyAttribute motion:motionTarget="@+id/svg_image_view" motion:framePosition="0" android:translationY="-1000px"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image_view" motion:framePosition="100" android:translationY="0px"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image2_view" motion:framePosition="0" android:translationY="-1000px"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image2_view" motion:framePosition="100" android:translationY="0px"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image3_view" motion:framePosition="0" android:scaleX="0.7"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image3_view" motion:framePosition="0" android:scaleX="15"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image3_view" motion:framePosition="0" android:scaleY="0.7"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image3_view" motion:framePosition="40" android:scaleX="15"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image3_view" motion:framePosition="40" android:scaleY="15"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image3_view" motion:framePosition="100" android:scaleX="15"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image3_view" motion:framePosition="100" android:scaleX="50"/>
            <KeyAttribute motion:motionTarget="@+id/svg_image3_view" motion:framePosition="100" android:scaleY="50"/>
            <KeyAttribute motion:motionTarget="@+id/text_view" motion:framePosition="50" android:alpha="0"/>
            <KeyAttribute motion:motionTarget="@+id/text_view" motion:framePosition="80" android:alpha="1"/>
            <KeyAttribute motion:motionTarget="@+id/text_view" motion:framePosition="50" android:translationY="20dp"/>
            <KeyAttribute motion:motionTarget="@+id/text_view" motion:framePosition="80" android:translationY="0dp"/>
        </KeyFrameSet>
        <OnSwipe motion:maxAcceleration="10000"/>
    </Transition>
    <ConstraintSet
        android:id="@+id/start">
        <Constraint
            android:id="@+id/svg_image_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:translationY="-1000px"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"/>
        <Constraint
            android:id="@+id/svg_image2_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:translationY="-1000px"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"/>
        <Constraint android:id="@+id/svg_image3_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:rotation="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"
            android:scaleX="0.7"
            android:scaleY="0.7"/>
        <Constraint
            android:id="@+id/text_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:translationY="20dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.5"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="294dp" android:layout_marginTop="361dp"/>
    </ConstraintSet>
    <ConstraintSet
        android:id="@+id/end">
        <Constraint
            android:id="@+id/svg_image_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:translationY="1000px"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"/>
        <Constraint
            android:id="@+id/svg_image2_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:translationY="10000px"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:id="@+id/svg_image3_view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:rotation="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.133"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="166dp"
            android:scaleX="0.7"
            android:scaleY="0.7"/>
        <Constraint
            android:id="@+id/text_view"
            motion:layout_constraintEnd_toEndOf="@+id/svg_image3_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:translationY="0dp"
            motion:layout_constraintBottom_toBottomOf="@+id/svg_image3_view"
            motion:layout_constraintTop_toBottomOf="@+id/svg_image3_view"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="@+id/svg_image3_view"/>
    </ConstraintSet>
</MotionScene>